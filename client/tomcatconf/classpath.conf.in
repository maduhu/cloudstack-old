#!/bin/bash

SYSTEMJARS="@SYSTEMJARS@"
SCP=$(build-classpath $SYSTEMJARS 2>/dev/null) ; if [ $? != 0 ] ; then export SCP="@SYSTEMCLASSPATH@" ; fi
MCP="@MSCLASSPATH@"
PCP="@PREMIUMCLASSPATH@"
DCP="@DEPSCLASSPATH@"
CLASSPATH=$PCP:$SCP:$DCP:$MCP:@MSCONF@
export CLASSPATH
PATH=/sbin:/usr/sbin:$PATH
export PATH
